<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<VBox alignment="CENTER" prefHeight="451.0" prefWidth="319.0" spacing="20.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.matt.controllers.RegisterController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <children>
      <Text fill="#898996" strokeType="OUTSIDE" strokeWidth="0.0" text="Register Account">
         <font>
            <Font name="Fira Code Bold" size="41.0" />
         </font>
      </Text>
      <AnchorPane prefHeight="315.0" prefWidth="404.0">
         <children>
            <TextField fx:id="nameField" layoutX="176.0" layoutY="14.0" prefHeight="24.0" prefWidth="154.0" />
            <Label layoutX="49.0" layoutY="18.0" text="Username" />
            <Label layoutX="51.0" layoutY="100.0" text="Password" />
            <Button fx:id="backButton" cancelButton="true" layoutX="14.0" layoutY="261.0" mnemonicParsing="false" onAction="#handleCancelButton" prefHeight="24.0" prefWidth="180.0" text="Back" />
            <Button fx:id="registerButton" defaultButton="true" layoutX="210.0" layoutY="261.0" mnemonicParsing="false" onAction="#handleRegisterButton" prefHeight="24.0" prefWidth="180.0" text="Register" />
